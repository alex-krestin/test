         <top>
            <ToolBar BorderPane.alignment="CENTER">
               <ImageView fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
                  <Image url="@../../../resources/images/dashboard.png"/>
               </ImageView>
               <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Dashboard" wrappingWidth="100.0">
                  <font>
                     <Font name="System Bold" size="12.0"/>
                  </font>
               </Text>
               <Button alignment="CENTER" contentDisplay="TOP" graphicTextGap="0.0" mnemonicParsing="false"
                       text="Contratti" onAction="#gotoContracts" textAlignment="CENTER">
